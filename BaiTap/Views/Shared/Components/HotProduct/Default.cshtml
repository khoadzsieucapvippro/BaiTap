@model List<string>

<div class="row">
    @foreach (var name in Model)
    {
        <div class="col-md-3">
            <div class="card border-danger mb-3">
                <img src="data:image/jpeg;base64," class="card-img-top" />
                <div class="card-body">
                    <h6 class="text-danger">@name</h6>
                </div>
            </div>
        </div>
    }
</div>
